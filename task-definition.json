{
    "family": "nginx",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "nginx1",
        "image": "321486094060.dkr.ecr.ap-northeast-1.amazonaws.com/githubactions-nginx-1:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      },
      {
        "name": "nginx2",
        "image": "321486094060.dkr.ecr.ap-northeast-1.amazonaws.com/githubactions-nginx-2:latest",
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "executionRoleArn": "ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512"
}